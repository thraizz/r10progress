    "antd": "^5.25.1",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts-for-react": "^3.0.2",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "jotai": "^2.12.4",
    "papaparse": "^5.5.2",
    "postcss": "^8.5.3",
    "react": "^18.3.1",
